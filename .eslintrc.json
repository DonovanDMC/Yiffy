{
  "extends": "@uwu-codes/eslint-config",
  "rules": {
    "@typescript-eslint/member-ordering": [
      "error",
      {
        "default": {
          "memberTypes": [
            "signature",
            "private-static-field",
            "protected-static-field",
            "static-field",
            "private-field",
            "protected-field",
            "field",
            "abstract-field",
            "private-abstract-method",
            "protected-abstract-method",
            "abstract-method",
            "private-constructor",
            "protected-constructor",
            "constructor",
            "private-static-method",
            "protected-static-method",
            "static-method",
            "private-method",
            "protected-method",
            "method",
            "private-get",
            "protected-get",
            "get",
            "private-set",
            "protected-set",
            "set"
          ],
          "order": "alphabetically"
        }
      }
    ]
  }
}

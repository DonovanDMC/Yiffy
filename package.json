    "api",
    "rest",
    "rest-api",
    "yiff",
    "yiff-api",
    "yiffy",
    "yiffer"
  ],
  "author": "Donovan_DMC <hewwo@yiff.rocks> (https://donovan.is.gay)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DonovanDMC/Yiffy/issues"
  },
  "homepage": "https://yiff.rest",
  "devDependencies": {
    "@types/node": "^17.0.5",
    "@uwu-codes/eslint-config": "^1.0.31",
    "@uwu-codes/tsconfig": "^1.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}

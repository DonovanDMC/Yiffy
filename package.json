{
  "name": "yiffy",
  "version": "2.0.9",
  "description": "API Wrapper for https://yiff.rest.",
  "main": "build/src/index.js",
  "scripts": {
    "add-re-export": "echo \"export * from \\\"./util/types\\\";\" >> build/src/index.d.ts",
    "build": "rm -rf build && tsc && cp src/util/types.d.ts build/src/util/ && npm run add-re-export && echo Build Finished",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FurryBotCo/Yiffy.git"
  },
  "keywords": [
    "furry",
    "furry-api",
    "api",
    "rest",
    "rest-api",
    "yiff",
    "yiff-api",
    "yiffy",
    "yiffer"
  ],
  "author": "Donovan_DMC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FurryBotCo/Yiffy/issues"
  },
  "homepage": "https://github.com/FurryBotCo/Yiffy#readme",
  "devDependencies": {
    "@types/node": "^14.14.20",
    "eslint": "^7.17.0",
    "eslint-plugin-deprecation": "^1.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.13",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
